#!/usr/bin/env bash

echo -e "Concept\tPositive\tNegative"
dm join -j 2 \
    <(dataset ontology-terms go \
        | grep 'positive regulation of' \
        | sed 's/positive regulation of \+//' \
        | dm sort -k2b,2) \
    <(dataset ontology-terms go \
        | grep 'negative regulation of' \
        | sed 's/negative regulation of \+//' \
        | dm sort -k2b,2)
